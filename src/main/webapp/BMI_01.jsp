	<%
	request.setCharacterEncoding("utf-8");
	String num1 = request.getParameter("cm");
	String num2 = request.getParameter("kg");
	int cm1=Integer.parseInt(num1);
	int kg2=Integer.parseInt(num2);
	
	double BMI=(double)(kg2/(cm1*cm1*0.0001));
	out.println("입력한 키는"+cm1+"이고 <br>");
	out.println("입력한 몸무게는"+kg2+"이므로 <br>");
	out.println("당신의 BMI 수치는"+BMI+"이며 <br>");
	String stateImage="";
	out.println("결과는 ");
	if(BMI>0&&BMI<=18.4){
		out.println("저체중입니다.");
		stateImage="data:image/png;base64,";
	}
	else if(BMI>=18.5&&BMI<23){
		out.println("정상체중입니다.");
	}
	else if(BMI>=23&&BMI<25){
		out.println("과체중입니다.");
	}
	else if(BMI>=25&&BMI<30){
		out.println("비만입니다.");
	}
	else if(BMI>=30){
		out.println("고도비만입니다.");
	}
	%>

	



</body>
